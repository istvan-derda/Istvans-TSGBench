test_run:
    python run_tts_gan.py --dataset_no 2 --exp_name D2_Testrun --max_iter 50


init:
    rm -rf TTS_GAN
    git clone git@github.com:istvan-derda/istvans-tts-gan.git TTS_GAN
    $(cd TTS_GAN && git fetch --tags && git checkout tags/TTS-GAN-Run1)


get-gen:
    rsync -avz --progress scuni:"Istvans-TSGBench/models/$(basename "$PWD")/gen" .
